<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace必须定义，非绑定到类或是接口的方式 -->
<mapper namespace="com.wardrobe.dao.IModelAdminDao">
    <!-- 查询所有季节信息 -->
	<select id="queryAllSeason" resultType="com.wardrobe.entity.Season" >
		select * from season
	</select>
	<!-- 查询所有衣服类型信息 -->
	<select id="queryAllType" resultType="java.util.Map" >
		select * from clothes_type
	</select>
</mapper>